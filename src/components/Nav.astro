<!--Navbar Starts-->
<header class="relative">
  <div class="max-w-7xl mx-auto px-4 sm:px-6">
    <div
      class="flex justify-between items-center border-b border-black-600 pt-6 pb-4 lg:px-16 sm:px-6 md:space-x-10">
      <div class="flex justify-start lg:w-0 lg:flex-1">
        <a
          href="/"
          class="text-2xl font-extrabold leading-none text-gray-900 select-none logo"
          >Prannay<span class="text-indigo-600">.</span></a
        >
      </div>
      <div class="-mr-2 -my-2 md:hidden">
        <button
          type="button"
          id="hamburger-menu"
          class="rounded-md p-2 inline-flex items-center justify-center text-black-900 hover:text-black-900 hover:bg-black-900 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-indigo-500"
          aria-expanded="false">
          <span class="sr-only">Open menu</span>
          <!-- Heroicon name: outline/menu -->
          <svg
            class="h-6 w-6"
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="2"
            stroke="currentColor"
            aria-hidden="true">
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              d="M4 6h16M4 12h16M4 18h16"></path>
          </svg>
        </button>
      </div>
      <nav class="hidden desktop-nav md:flex space-x-10">
        <a
          href="/about-me/"
          class="text-sm font-normal text-black-300 hover:text-indigo-900">
          About
        </a>
        <a
          href="/writee-ai/"
          class="text-sm font-normal text-black-300 hover:text-indigo-900">
          Writee AI
        </a>
        <a
          href="/projects/"
          class="text-sm font-normal text-black-300 hover:text-indigo-900">
          Projects
        </a>
        <a
          href="/blogs/"
          class="text-sm font-normal text-black-300 hover:text-indigo-900">
          Blog
        </a>
        <a
          href="https://drive.google.com/file/d/1Ad4bSgonObG4z2Y1GEtKN17Zkaphz8GJ/view?usp=sharing"
          target="”_blank”"
          class="text-sm font-normal text-black-300 hover:text-indigo-900">
          CV
        </a>
      </nav>
      <div class="hidden md:flex items-center justify-end md:flex-1 lg:w-0">
        <a
          href="/contact-me/"
          class="whitespace-nowrap inline-flex items-center justify-center px-4 py-2 border border-transparent rounded-full shadow-sm text-sm font-normal text-white bg-indigo-600 hover:bg-indigo-700">
          <div class="flex items-center">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              class="h-4 w-4"
              viewBox="0 0 20 20"
              fill="currentColor">
              <path
                fill-rule="evenodd"
                d="M18 5v8a2 2 0 01-2 2h-5l-5 4v-4H4a2 2 0 01-2-2V5a2 2 0 012-2h12a2 2 0 012 2zM7 8H5v2h2V8zm2 0h2v2H9V8zm6 0h-2v2h2V8z"
                clip-rule="evenodd"></path>
            </svg>
            <p class="inline-block pl-2">Contact Me</p>
          </div>
        </a>
      </div>
    </div>
  </div>

  <!-- Mobile Nav Links -->
  <div
    id="mobile-nav"
    class="hidden z-50 absolute top-0 inset-x-0 p-2 transition transform origin-top-right md:hidden">
    <div
      class="rounded-lg shadow-lg ring-1 ring-black ring-opacity-5 bg-white divide-y-2 divide-gray-50">
      <div class="pt-5 pb-6 px-5">
        <div class="flex items-center justify-between">
          <div>
            <a
              href="/"
              class="text-2xl font-extrabold leading-none text-gray-900 select-none logo"
              >Prannay<span class="text-indigo-600">.</span></a
            >
          </div>
          <div class="-mr-2">
            <button
              type="button"
              id="close-mobile-nav"
              class="bg-white rounded-md p-2 inline-flex items-center justify-center text-gray-400 hover:text-gray-500 hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-indigo-500">
              <span class="sr-only">Close menu</span>
              <svg
                class="h-6 w-6"
                xmlns="http://www.w3.org/2000/svg"
                fill="none"
                viewBox="0 0 24 24"
                stroke-width="2"
                stroke="currentColor"
                aria-hidden="true">
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  d="M6 18L18 6M6 6l12 12"></path>
              </svg>
            </button>
          </div>
        </div>
        <div class="mt-6">
          <nav class="grid mobile-nav gap-y-8">
            <a
              href="/about-me/"
              class="-m-3 p-3 flex items-center rounded-md hover:bg-gray-50">
